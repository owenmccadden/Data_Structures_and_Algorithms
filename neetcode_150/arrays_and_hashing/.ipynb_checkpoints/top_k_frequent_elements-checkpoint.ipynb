    "from collections import Counter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def top_k_frequent(nums, k):\n",
    "    counts = Counter(nums)\n",
    "    return sorted(counts, key=lambda x: counts[x])[::-1][:k]"
   ]
  },
