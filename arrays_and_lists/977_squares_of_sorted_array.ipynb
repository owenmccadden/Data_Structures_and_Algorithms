    "# 977 Squares of Sorted Array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sortedSquares(nums):\n",
    "        for i in range(len(nums)):\n",
    "            nums[i] = pow(nums[i], 2)\n",
    "        nums.sort()\n",
    "        return nums"
   ]
  },
